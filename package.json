    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@types/node": "18.7.9",
    "bumpp": "8.2.1",
    "eslint": "8.22.0",
    "pnpm": "7.9.3",
    "rimraf": "3.0.2",
    "tsup": "6.2.2",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vitest": "0.22.1"
  }
}
